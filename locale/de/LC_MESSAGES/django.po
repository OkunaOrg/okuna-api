msgid "email address"
msgstr "E-Mail-Adresse"

#: openbook_auth/models.py:61 openbook_communities/models.py:62
msgid "is deleted"
msgstr "ist gelöscht"

#: openbook_auth/models.py:66 openbook_invitations/models.py:26
msgid "username"
msgstr "Benutzername"

