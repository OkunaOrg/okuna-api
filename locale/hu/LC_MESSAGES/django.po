#: openbook_auth/checkers.py:27
msgid "You cannot edit a closed post"
msgstr "Nem szerkeszthetsz egy zárt bejegyzést"

#: openbook_auth/checkers.py:35
#, python-format
msgid "You cannot post to circle with id %(id)s"
msgstr "Nem posztolhatsz ehhez a körhöz, id %(id)s"

#: openbook_auth/checkers.py:42
msgid "You cannot post to a community you're not member of "
msgstr "Nem posztolhatsz egy közösségben, melynek nem vagy tagja "

#: openbook_auth/checkers.py:50
msgid "Only moderators/administrators can enable/disable comments"
msgstr "Csak moderátorok/adminisztrátorok engedélyezhetik illetve tilthatják le a hozzászólásokat"

#: openbook_auth/checkers.py:60
msgid "Comments are disabled for this post"
msgstr "A hozzászólások le vannak tiltva ezen a bejegyzésen"

#: openbook_auth/checkers.py:69
msgid "Only public posts can be translated"
msgstr "Csak nyilvános bejegyzések fordíthatóak le"

#: openbook_auth/checkers.py:73
msgid "Post has no text to be translated"
msgstr "A bejegyzés nem tartalmaz lefordítható szöveget"

#: openbook_auth/checkers.py:77
msgid "Post has no assigned language to be able to translate"
msgstr "A bejegyzés nem rendelkezik megadott nyelvvel, így nem fordítható le"

#: openbook_auth/checkers.py:81 openbook_auth/checkers.py:771
msgid "User's preferred translation language not set"
msgstr "A felhasználó nyelvi beállításai nincsenek megadva"

#: openbook_auth/checkers.py:90 openbook_auth/checkers.py:102
msgid "Only community posts can be opened/closed"
msgstr "Csak közösségi bejegyzések nyithatóak meg/zárhatóak be"

#: openbook_auth/checkers.py:95
msgid "Only administrators/moderators can open this post"
msgstr "Csak adminisztrátorok/moderátorok nyithatják meg ezt a bejegyzést"

#: openbook_auth/checkers.py:107
msgid "Only administrators/moderators can close this post"
msgstr "Csak adminisztrátorok/moderátorok zárhatják be ezt a bejegyzést"

#: openbook_auth/checkers.py:135
msgid "Already following user."
msgstr "Már követed ezt a felhasználót."

#: openbook_auth/checkers.py:142
msgid "Maximum number of follows reached."
msgstr "Elérted a maximális követések számát."

#: openbook_auth/checkers.py:151
msgid "Already following user in list."
msgstr "Már követed a felhasználót ebben a listában."

#: openbook_auth/checkers.py:160
msgid "Not following user in list."
msgstr "Nem követed a felhasználót ebben a listában."

#: openbook_auth/checkers.py:167
msgid "Not following user."
msgstr "Nem követed a felhasználót."

#: openbook_auth/checkers.py:174
msgid "Maximum number of connections reached."
msgstr "Elérted a maximális kapcsolatok számát."

#: openbook_auth/checkers.py:187
msgid "Already connected with user."
msgstr "A felhasználó már a kapcsolataid között van."

#: openbook_auth/checkers.py:194
msgid "Already fully connected with user."
msgstr "A felhasználó már teljesen a kapcsolataid között van."

#: openbook_auth/checkers.py:201
msgid "Not connected with user."
msgstr "A felhasználó nincs kapcsolataid között."

#: openbook_auth/checkers.py:208
msgid "Not connected with user in given circle."
msgstr "A felhasználó nincs a kapcsolataid között a megadott körben."

#: openbook_auth/checkers.py:215
msgid "Already connected with user in given circle."
msgstr "A felhasználó már a kapcsolataid között van a megadott körben."

#: openbook_auth/checkers.py:222
msgid "List does not exist."
msgstr "A lista nem létezik."

#: openbook_auth/checkers.py:229
msgid "Circle does not exist."
msgstr "A kör nem létezik."

#: openbook_auth/checkers.py:236
msgid "One or more of the circles do not exist."
msgstr "Egy vagy több kör nem létezik."

#: openbook_auth/checkers.py:249
msgid "Only moderators/administrators can remove community posts."
msgstr "Csak moderátorok/adminisztrátorok törölhetnek bejegyzéseket egy közösségben."

#: openbook_auth/checkers.py:257
msgid "You cannot remove a post that does not belong to you"
msgstr "Nem törölhetsz egy bejegyzést amit nem te írtál"

#: openbook_auth/checkers.py:264
msgid "Can't delete a list that does not belong to you."
msgstr "Nem törölhetsz egy listát amely nem hozzád tartozik."

#: openbook_auth/checkers.py:271
msgid "Can't update a list that does not belong to you."
msgstr "Nem frissíthetsz egy listát amely nem hozzád tartozik."

#: openbook_auth/checkers.py:278
msgid "Can't delete a community that you do not administrate."
msgstr "Nem törölhetsz egy közösséget, melynek nem vagy az adminisztrátora."

#: openbook_auth/checkers.py:285
msgid "Can't update a community that you do not administrate."
msgstr "Nem frissíthetsz egy közösséget, melynek nem vagy az adminisztrátora."

#: openbook_auth/checkers.py:296
msgid "The community is private. You must become a member to retrieve its posts."
msgstr "A közösség zárt. Ahhoz hogy megjelenítsd a bejegyzéseit, tagnak kell lenned."

#: openbook_auth/checkers.py:304
msgid "Only administrators/moderators can view closed posts"
msgstr "Csak adminisztrátorok/moderátorok tekinthetnek meg zárt bejegyzéseket"

#: openbook_auth/checkers.py:316
msgid "Only members can enable new post notifications"
msgstr ""

#: openbook_auth/checkers.py:325 openbook_auth/checkers.py:1390
msgid "New post notifications are already enabled"
msgstr ""

#: openbook_auth/checkers.py:341 openbook_auth/checkers.py:1404
msgid "You are not subscribed to new post notifications"
msgstr ""

#: openbook_auth/checkers.py:353
msgid "Can't see the members of a private community."
msgstr "Nem tekintheted meg egy zárt közösség tagjait."

#: openbook_auth/checkers.py:363
msgid "You are already a member of the community."
msgstr "Már tagja vagy ennek a közösségnek."

#: openbook_auth/checkers.py:370
msgid "You are not invited to join this community."
msgstr "Nem hívtak meg téged, hogy csatlakozz ehhez a közösséghez."

#: openbook_auth/checkers.py:377
msgid "You cannot leave a community you're not part of."
msgstr "Nem hagyhatsz el egy közösséget, melynek nem vagy tagja."

#: openbook_auth/checkers.py:382
msgid "You cannot leave a community you created."
msgstr "Nem hagyhatsz el egy közösséget, melyet te hoztál létre."

#: openbook_auth/checkers.py:389
msgid "You can only invite people to a community you are member of."
msgstr "Csak olyan közösségekbe hívhatsz meg embereket, amelyeknek tagja vagy."

#: openbook_auth/checkers.py:394
msgid "You have already invited this user to join the community."
msgstr "Már meghívtad ezt a felhasználót a közösségbe."

#: openbook_auth/checkers.py:402
msgid "The user is already part of the community."
msgstr "A felhasználó már a közösség tagja."

#: openbook_auth/checkers.py:410
msgid "Invites for this community are not enabled. Only administrators & moderators can invite."
msgstr "A meghívók nincsenek engedélyezve ebben a közösségben. Csak adminisztrátorok és moderátorok hívhatnak meg."

#: openbook_auth/checkers.py:418
msgid "No invite to withdraw."
msgstr "Nincs kiküldhető meghívó."

#: openbook_auth/checkers.py:427
msgid "Only community administrators & moderators can get banned users."
msgstr "Csak a közösség moderátorai és adminisztrátorai tekinthetik meg a letiltott felhasználókat."

#: openbook_auth/checkers.py:436 openbook_auth/checkers.py:459
msgid "Only community administrators & moderators can ban community members."
msgstr "Csak a közösség moderátorai és adminisztrátorai tilthatnak ki felhasználókat."

#: openbook_auth/checkers.py:443
msgid "User is already banned"
msgstr "A felhasználó már le van tiltva"

#: openbook_auth/checkers.py:450
msgid "You can't ban moderators or administrators of the community"
msgstr "Nem tilthatod le a közösség moderátorait vagy adminisztrátorait"

#: openbook_auth/checkers.py:466
msgid "Can't unban a not-banned user."
msgstr "Nem törölheted a tiltást egy le nem tiltott felhasználóról."

#: openbook_auth/checkers.py:473
msgid "Only the creator of the community can add other administrators."
msgstr "Csak a közösség létrehozója nevezhet ki adminisztrátorokat."

#: openbook_auth/checkers.py:481
msgid "User is already an administrator."
msgstr "A felhasználó már adminisztrátor."

#: openbook_auth/checkers.py:487
msgid "Can't make administrator a user that is not part of the community."
msgstr "Nem nevezhetsz ki adminisztrátornak egy felhasználót, aki nem része a közösségnek."

#: openbook_auth/checkers.py:494
msgid "Only the creator of the community can remove other administrators."
msgstr "Csak a közösség létrehozója törölhet adminisztrátorokat."

#: openbook_auth/checkers.py:502
msgid "User to remove is not an administrator."
msgstr "A megadott felhasználó nem egy adminisztrátor."

#: openbook_auth/checkers.py:513
msgid "Only administrators of the community can add other moderators."
msgstr "Csak a közösség adminisztrátorai nevezhetnek ki moderátorokat."

#: openbook_auth/checkers.py:521
msgid "User is an administrator."
msgstr "A felhasználó nem adminisztrátor."

#: openbook_auth/checkers.py:527
msgid "User is already a moderator."
msgstr "A felhasználó már moderátor."

#: openbook_auth/checkers.py:533
msgid "Can't make moderator a user that is not part of the community."
msgstr "Nem nevezhetsz ki moderátornak egy felhasználót, aki nem része a közösségnek."

#: openbook_auth/checkers.py:540
msgid "Only administrators of the community can remove other moderators."
msgstr "Csak a közösség adminisztrátorai törölhetnek moderátorokat."

#: openbook_auth/checkers.py:547
msgid "User to remove is not an moderator."
msgstr "A megadott felhasználó nem egy moderátor."

#: openbook_auth/checkers.py:567
msgid "Can't update a circle that does not belong to you."
msgstr "Nem frissíthetsz egy kört amely nem hozzád tartozik."

#: openbook_auth/checkers.py:572
msgid "Can't update the world circle."
msgstr "Nem sikerült frissíteni a világ kört."

#: openbook_auth/checkers.py:577
msgid "Can't update the connections circle."
msgstr "Nem sikerült frissíteni a kapcsolatok kört."

#: openbook_auth/checkers.py:584
msgid "Can't delete a circle that does not belong to you."
msgstr "Nem törölhetsz egy kört amely nem hozzád tartozik."

#: openbook_auth/checkers.py:589
msgid "Can't delete the world circle."
msgstr "Nem törölheted a világ kört."

#: openbook_auth/checkers.py:594
msgid "Can't delete the connections circle."
msgstr "Nem törölheted a kapcsolatok kört."

#: openbook_auth/checkers.py:601
msgid "Can't view a circle that does not belong to you."
msgstr "Nem tekinthetsz meg egy kört amely nem hozzád tartozik."

#: openbook_auth/checkers.py:608
msgid "Can't view a list that does not belong to you."
msgstr "Nem tekinthetsz meg egy listát amely nem hozzád tartozik."

#: openbook_auth/checkers.py:615
msgid "You already have a circle with that name."
msgstr "Már létezik ilyen nevű kör."

#: openbook_auth/checkers.py:622
msgid "You already have a list with that name."
msgstr "Már létezik ilyen nevű lista."

#: openbook_auth/checkers.py:634
msgid "A community with that name already exists."
msgstr "Már létezik ilyen nevű közösség."

#: openbook_auth/checkers.py:641
msgid "You must be member of a community before making it a favorite."
msgstr "Tagja kell lenned egy közösségnek mielőtt hozzáadnád azt a kedvenceidhez."

#: openbook_auth/checkers.py:646
msgid "You have already marked this community as favorite."
msgstr "Már kedvencnek jelölted ezt a közösséget."

#: openbook_auth/checkers.py:653
msgid "You have already marked this community as excluded."
msgstr ""

#: openbook_auth/checkers.py:658
msgid "Private communities are always excluded from top posts."
msgstr ""

#: openbook_auth/checkers.py:665
msgid "You have not marked this community as excluded."
msgstr ""

#: openbook_auth/checkers.py:672
msgid "You have not favorited the community."
msgstr "Nem jelölted kedvencnek ezt a közösséget."

#: openbook_auth/checkers.py:679
msgid "You cannot mark as read a notification that doesn't belong to you."
msgstr "Nem jelölhetsz olvasottnak olyan értesítéseket, melyek nem hozzád tartoznak."

#: openbook_auth/checkers.py:690
msgid "This notification does not belong to you."
msgstr "Ez az értesítés nem a tiéd."

#: openbook_auth/checkers.py:709
msgid "Device not found"
msgstr "Eszköz nem található"

#: openbook_auth/checkers.py:716
msgid "Post already muted"
msgstr "A bejegyzés már le van némítva"

#: openbook_auth/checkers.py:729
msgid "Post is not muted"
msgstr "A bejegyzés nincs némítva"

#: openbook_auth/checkers.py:736
msgid "Post comment already muted"
msgstr "A hozzászólás már le van némítva"

#: openbook_auth/checkers.py:750
msgid "Post_comment is not muted"
msgstr "A hozzászólás nincs némítva"

#: openbook_auth/checkers.py:759
msgid "Only public post comments can be translated"
msgstr "Csak nyilvános hozzászólások szerkeszthetőek"

#: openbook_auth/checkers.py:763
msgid "Post comment has no text to be translated"
msgstr "A hozzászólás nem tartalmaz lefordítható szöveget"

#: openbook_auth/checkers.py:767
msgid "Post comment has no assigned language to be able to translate"
msgstr "A hozzászólás nem rendelkezik megadott nyelvvel, így nem fordítható le"

#: openbook_auth/checkers.py:778
msgid "This post does not belong to you."
msgstr "Ez a bejegyzés nem a tiéd."

#: openbook_auth/checkers.py:785
msgid "Wrong password."
msgstr "Hibás jelszó."

#: openbook_auth/checkers.py:811
msgid "You cannot block yourself."
msgstr "Nem tudod letiltani saját magad."

#: openbook_auth/checkers.py:817
msgid "You cannot unblock an account you have not blocked."
msgstr "Nem törölheted a tiltást olyan fiókról, amelyet le se tiltottál."

#: openbook_auth/checkers.py:825 openbook_auth/checkers.py:1386
#: openbook_auth/checkers.py:1400
msgid "This account is blocked."
msgstr "Ez a fiók le van tiltva."

#: openbook_auth/checkers.py:834
msgid "You cannot report your own comment."
msgstr "Nem jelentheted a saját hozzászólásodat."

#: openbook_auth/checkers.py:841
msgid "You have already reported the comment."
msgstr "Már jelentetted ezt a hozzászólást."

#: openbook_auth/checkers.py:850
msgid "You cannot report your own post."
msgstr "Nem jelentheted a saját bejegyzésedet."

#: openbook_auth/checkers.py:857
msgid "You have already reported the post."
msgstr "Már jelentetted ezt a bejegyzést."

#: openbook_auth/checkers.py:865
msgid "You cannot report yourself..."
msgstr "Nem jelentheted saját magad..."

#: openbook_auth/checkers.py:872
msgid "You have already reported the user."
msgstr "Már jelentetted ezt a felhasználót."

#: openbook_auth/checkers.py:880
msgid "You cannot report your own community."
msgstr "Nem jelentheted a saját közösségedet."

#: openbook_auth/checkers.py:887
msgid "You have already reported the community."
msgstr "Már jelentetted ezt a közösséget."

#: openbook_auth/checkers.py:898
msgid "You have already reported the hashtag."
msgstr ""

