#: openbook_auth/models.py:168
msgid "Token type does not match"
msgstr "A token típusa nem egyezik"

#: openbook_auth/checkers.py:917 openbook_auth/checkers.py:959
msgid "Token user id does not match"
msgstr "A token felhasználó id nem egyezik"

#: openbook_auth/checkers.py:922 openbook_auth/checkers.py:964
#: openbook_auth/models.py:175
msgid "Invalid token signature"
msgstr "Érvénytelen token aláírás"

#: openbook_auth/checkers.py:926 openbook_auth/checkers.py:968
#: openbook_auth/models.py:179
msgid "Token expired"
msgstr "A token lejárt"

#: openbook_auth/checkers.py:930 openbook_auth/checkers.py:972
#: openbook_auth/models.py:183
msgid "Failed to decode token"
msgstr "Nem sikerült dekódolni a tokent"

#: openbook_auth/checkers.py:934 openbook_auth/checkers.py:976
#: openbook_auth/models.py:191
msgid "Invalid token"
msgstr "Érvénytelen token"

#: openbook_auth/checkers.py:954
msgid "Token email does not match"
msgstr "A token e-mail cím nem egyezik"

#: openbook_auth/checkers.py:990
msgid "Can't connect in the world circle."
msgstr "Nem sikerült kapcsolódni a világ körhöz."

#: openbook_auth/checkers.py:1002
msgid "The email is already taken."
msgstr "Az e-mail cím már használatban van."

#: openbook_auth/checkers.py:1014 openbook_auth/validators.py:18
msgid "The username is already taken."
msgstr "A felhasználónév már foglalt."

#: openbook_auth/checkers.py:1025 openbook_auth/checkers.py:1054
msgid "The comment does not belong to the specified post."
msgstr "A hozzászólás nem a megadott bejegyzéshez tartozik."

#: openbook_auth/checkers.py:1033
msgid "Only administrators/moderators can edit a closed post."
msgstr "Csak adminisztrátorok/moderátorok szerkeszthetnek egy zárt bejegyzést."

#: openbook_auth/checkers.py:1041
msgid "You cannot edit a comment that does not belong to you"
msgstr "Nem szerkeszthetsz egy hozzászólást amit nem te írtál"

#: openbook_auth/checkers.py:1064
msgid "Only moderators/administrators can remove closed community posts."
msgstr "Csak moderátorok/adminisztrátorok törölhetnek zárt bejegyzéseket egy közösségben."

#: openbook_auth/checkers.py:1068 openbook_auth/checkers.py:1082
msgid "You cannot remove a comment that does not belong to you"
msgstr "Nem törölhetsz egy hozzászólást amit nem te írtál"

#: openbook_auth/checkers.py:1093 openbook_auth/checkers.py:1106
msgid "No comment found with given id for post with given uuid"
msgstr "Nem található hozzászólás a megadott id-ra a megadott uuid-vel rendelkező bejegyzésben"

#: openbook_auth/checkers.py:1112
msgid "You can post a reply to a comment, not to an existing reply"
msgstr "Választ csak hozzászólásokhoz írhatsz, nem pedig egy már létező válaszhoz"

#: openbook_auth/checkers.py:1124
msgid "That reaction does not belong to the specified post."
msgstr "A reakció nem a megadott bejegyzéshez tartozik."

#: openbook_auth/checkers.py:1130
msgid "Can't delete a reaction that does not belong to you."
msgstr "Nem törölhetsz egy reakciót amely nem hozzád tartozik."

#: openbook_auth/checkers.py:1147
msgid "Not a valid emoji to react with"
msgstr "A reagáláshoz megadott emoji érvénytelen"

#: openbook_auth/checkers.py:1158
msgid "This post is private."
msgstr "Ez a bejegyzés privát."

#: openbook_auth/checkers.py:1165
msgid "This hashtag is not  available."
msgstr ""

#: openbook_auth/checkers.py:1175
msgid "Cant react to comments on a closed post."
msgstr "Nem reagálhatsz hozzászólásokhoz egy zárt bejegyzésen."

#: openbook_auth/checkers.py:1184
msgid "Can't delete a comment reaction that does not belong to you."
msgstr "Nem törölhetsz egy hozzászólás-reakciót amely nem hozzád tartozik."

#: openbook_auth/checkers.py:1193
msgid "This comment is private."
msgstr "Ez a hozzászólás privát."

#: openbook_auth/checkers.py:1219
msgid "You have already reported the moderated_object."
msgstr "Már jelentetted ezt a moderált tárgyat."

#: openbook_auth/checkers.py:1228
msgid "The moderated object has been verified and can no longer be edited."
msgstr "A moderált tárgy ellenőrizve lett és nem szerkeszthető többé."

#: openbook_auth/checkers.py:1233
msgid "The moderated object has already been approved/rejected."
msgstr "A moderált tárgy már jóvá volt hagyva vagy vissza lett utasítva."

#: openbook_auth/checkers.py:1242 openbook_auth/checkers.py:1250
msgid "The moderated object has already been verified."
msgstr "A moderált tárgy már ellenőrizve van."

#: openbook_auth/checkers.py:1258
msgid "The moderated object has not been verified."
msgstr "A moderált tárgy még nincs ellenőrizve."

#: openbook_auth/checkers.py:1266
msgid "The moderated object is already verified."
msgstr "A moderált tárgy már ellenőrizve van."

#: openbook_auth/checkers.py:1271
msgid "You cannot verify a moderated object with status pending. Please approve or reject it."
msgstr "Nem ellenőrizhetsz egy függőben levő moderált tárgyat. Kérünk, hagyd jóvá vagy mellőzd."

#: openbook_auth/checkers.py:1289
msgid "Only community staff can moderate community posts"
msgstr "Csak a közösség staffja moderálhatja a bejegyzéseket"

#: openbook_auth/checkers.py:1291
msgid "Only global moderators can moderate non-community posts"
msgstr "Csak globális moderátorok moderálhatnak bejegyzéseket, melyek nem egy közösségen belül lettek létrehozva"

#: openbook_auth/checkers.py:1295
msgid "Only community staff can moderate community post comments"
msgstr "Csak a közösség staffja moderálhatja a hozzászólásokat"

#: openbook_auth/checkers.py:1297
msgid "Only global moderators can moderate non-community post comments"
msgstr "Csak globális moderátorok moderálhatnak hozzászólásokat, melyek nem egy közösségen belül lettek létrehozva"

#: openbook_auth/checkers.py:1299
msgid "Non global moderators can only moderate posts and post comments."
msgstr "A nem-globális moderátorok csak bejegyzéseket és hozzászólásokat moderálhatnak."

